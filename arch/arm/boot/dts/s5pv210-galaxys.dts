/dts-v1/;
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/sound/audio-jack-events.h>
#include "s5pv210-aries.dtsi"

/ {
	model = "Samsung Galaxy S1 (GT-I9000) based on S5PV210";
	compatible = "samsung,galaxys", "samsung,aries", "samsung,s5pv210";

	chosen {
		stdout-path = &uart2;
	};

	nand_pwrseq: nand-pwrseq {
		compatible = "mmc-pwrseq-simple";
		reset-gpios = <&gpj2 7 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default";
		pinctrl-0 = <&massmemory_en>;
	};

	gpio-keys {
		compatible = "gpio-keys";

		power {
			label = "power";
			gpios = <&gph2 6 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_POWER>;
			wakeup-source;
		};

		vol-down {
			label = "volume_down";
			gpios = <&gph3 1 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_VOLUMEDOWN>;
		};

		vol-up {
			label = "volume_up";
			gpios = <&gph3 2 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_VOLUMEUP>;
		};

		home {
			label = "home";
			gpios = <&gph3 5 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_HOME>;
			wakeup-source;
		};
	};

	modem {
		compatible = "samsung,onedram-modemctl";

		interrupt-parent = <&gph1>;
		interrupt-names = "active", "onedram";
		interrupts = <7 IRQ_TYPE_EDGE_BOTH>, <3 IRQ_TYPE_LEVEL_LOW>;
		reg = <0x35000000 0x01000000>;

		phone_active-gpio = <&gph1 7 GPIO_ACTIVE_HIGH>;
		pda_active-gpio = <&mp03 3 GPIO_ACTIVE_HIGH>;
		cp_reset-gpio = <&gph1 0 GPIO_ACTIVE_HIGH>;

		num-pdp-contexts = <3>;

		pinctrl-0 = <&onedram_int &eint15 &cp_rst &pda_active>;
		pinctrl-names = "default";
	};

	shared_micbias_reg: regulator-shared-micbias {
		compatible = "regulator-fixed";
		regulator-name = "Shared_Micbias";
		gpio = <&gpj4 2 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	};

	sound {
		compatible = "samsung,aries-wm8994";

		i2s-controller = <&i2s0>;
		audio-codec = <&wm8994>;

		samsung,modem-format = "dsp_a";
		samsung,modem-bitclock-inversion;
		samsung,modem-bitclock-master;
		samsung,modem-frame-master;

		main-micbias-supply = <&shared_micbias_reg>;
		headset-micbias-supply = <&shared_micbias_reg>;

		io-channels = <&adc 3>;
		io-channel-names = "headset-detect";
		headset-detect-gpios = <&gph0 6 GPIO_ACTIVE_HIGH>;
		headset-key-gpios = <&gph3 6 GPIO_ACTIVE_HIGH>;

		dock-extcon = <&fsa9480>;

		model = "Aries";
		samsung,audio-routing =
			"HP", "HPOUT1L",
			"HP", "HPOUT1R",

			"SPK", "SPKOUTLN",
			"SPK", "SPKOUTLP",

			"RCV", "HPOUT2N",
			"RCV", "HPOUT2P",

			"LINE", "LINEOUT2N",
			"LINE", "LINEOUT2P",

			"IN1LP", "Main Mic",
			"IN1LN", "Main Mic",

			"IN1RP", "Headset Mic",
			"IN1RN", "Headset Mic",

			"IN2LN", "FM In",
			"IN2RN", "FM In";

		pinctrl-names = "default";
		pinctrl-0 = <&jack_det_irq &media_key_irq>;
	};
};

&pinctrl0 {
	cp_rst: cp-rst {
		samsung,pins = "gph1-0";
		samsung,pin-pud = <S3C64XX_PIN_PULL_NONE>;
	};

	massmemory_en: massmemory-en {
		samsung,pins = "gpj2-7";
		samsung,pin-function = <EXYNOS_PIN_FUNC_OUTPUT>;
		samsung,pin-pud = <S3C64XX_PIN_PULL_NONE>;
		samsung,pin-drv = <EXYNOS4_PIN_DRV_LV1>;
	};

	onedram_int: onedram-int {
		samsung,pins = "gph1-3";
		samsung,pin-function = <EXYNOS_PIN_FUNC_F>;
		samsung,pin-pud = <S3C64XX_PIN_PULL_UP>;
	};

	pda_active: pda-active {
		samsung,pins = "mp03-3";
		samsung,pin-pud = <S3C64XX_PIN_PULL_NONE>;
	};

	jack_det_irq: jack-det-irq {
		samsung,pins = "gph0-6";
		samsung,pin-function = <EXYNOS_PIN_FUNC_EINT>;
	};

	media_key_irq: media-key-irq {
		samsung,pins = "gph3-6";
		samsung,pin-function = <EXYNOS_PIN_FUNC_EINT>;
	};
};

&sdhci0 {
	bus-width = <4>;
	non-removable;
	mmc-pwrseq = <&nand_pwrseq>;
	pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_bus4>;
	pinctrl-names = "default";
	status = "okay";
};

&uart3 {
	status = "okay";
};
