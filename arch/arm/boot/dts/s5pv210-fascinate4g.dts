/dts-v1/;
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/sound/audio-jack-events.h>
#include "s5pv210-aries.dtsi"

/ {
	model = "Samsung Galaxy S Fascinate 4G (SGH-T959P) based on S5PV210";
	compatible = "samsung,fascinate4g", "samsung,aries", "samsung,s5pv210";

	chosen {
		stdout-path = &uart2;
	};

	gpio-keys {
		compatible = "gpio-keys";

		power {
			label = "power";
			gpios = <&gph2 6 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_POWER>;
			wakeup-source;
		};

		vol-down {
			label = "volume_down";
			gpios = <&gph3 2 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_VOLUMEDOWN>;
		};

		vol-up {
			label = "volume_up";
			gpios = <&gph3 1 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_VOLUMEUP>;
		};
	};

	modem {
		compatible = "samsung,onedram-modemctl";

		interrupt-parent = <&gph1>;
		interrupt-names = "active", "onedram";
		interrupts = <7 IRQ_TYPE_EDGE_BOTH>, <3 IRQ_TYPE_LEVEL_LOW>;
		reg = <0x35000000 0x01000000>;

		phone_active-gpio = <&gph1 7 GPIO_ACTIVE_HIGH>;
		pda_active-gpio = <&mp03 3 GPIO_ACTIVE_HIGH>;
		cp_reset-gpio = <&gph1 0 GPIO_ACTIVE_HIGH>;
		int_resout-gpio = <&gph1 4 GPIO_ACTIVE_HIGH>;
		cp_pwr_rst-gpio = <&gph1 1 GPIO_ACTIVE_HIGH>;
		phone_on-gpio = <&gpj1 0 GPIO_ACTIVE_HIGH>;

		cp_rtc-supply = <&ldo6_reg>;
		cp_32khz-supply = <&cp32khz_reg>;

		ste-modem;
		num-pdp-contexts = <3>;

		pinctrl-0 = <&onedram_int &int_resout &cp_pwr_rst &eint15 &phone_on &cp_rst &pda_active>;
		pinctrl-names = "default";
	};

	main_micbias_reg: regulator-main-micbias {
		compatible = "regulator-fixed";
		regulator-name = "Main_Micbias";
		gpio = <&gpj4 2 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	};

	headset_micbias_reg: regulator-headset-micbias {
		compatible = "regulator-fixed";
		regulator-name = "Headset_Micbias";
		gpio = <&gpj2 5 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	};

	sound {
		compatible = "samsung,aries-wm8994";

		i2s-controller = <&i2s0>;
		audio-codec = <&wm8994>;

		samsung,modem-format = "dsp_a";
		samsung,modem-bitclock-inversion;

		main-micbias-supply = <&main_micbias_reg>;
		headset-micbias-supply = <&headset_micbias_reg>;

		io-channels = <&adc 3>;
		io-channel-names = "headset-detect";
		headset-detect-gpios = <&gph0 6 GPIO_ACTIVE_HIGH>;
		headset-key-gpios = <&gph3 6 GPIO_ACTIVE_HIGH>;

		dock-extcon = <&fsa9480>;

		model = "Fascinate4G";
		samsung,audio-routing =
			"HP", "HPOUT1L",
			"HP", "HPOUT1R",

			"SPK", "SPKOUTLN",
			"SPK", "SPKOUTLP",

			"RCV", "HPOUT2N",
			"RCV", "HPOUT2P",

			"LINE", "LINEOUT2N",
			"LINE", "LINEOUT2P",

			"IN1LP", "Main Mic",
			"IN1LN", "Main Mic",

			"IN1RP", "Headset Mic",
			"IN1RN", "Headset Mic";

		pinctrl-names = "default";
		pinctrl-0 = <&jack_det_irq &media_key_irq>;
	};
};

&pinctrl0 {
	onedram_int: onedram-int {
		samsung,pins = "gph1-3";
		samsung,pin-function = <EXYNOS_PIN_FUNC_F>;
		samsung,pin-pud = <S3C64XX_PIN_PULL_UP>;
	};

	int_resout: int-resout {
		samsung,pins = "gph1-4";
		samsung,pin-function = <EXYNOS_PIN_FUNC_F>;
		samsung,pin-pud = <S3C64XX_PIN_PULL_NONE>;
	};

	cp_pwr_rst: cp-pwr-rst {
		samsung,pins = "gph1-1";
		samsung,pin-function = <EXYNOS_PIN_FUNC_F>;
		samsung,pin-pud = <S3C64XX_PIN_PULL_NONE>;
	};

	phone_on: phone-on {
		samsung,pins = "gpj1-0";
		samsung,pin-function = <EXYNOS_PIN_FUNC_OUTPUT>;
		samsung,pin-pud = <S3C64XX_PIN_PULL_NONE>;
		samsung,pin-val = <0>;
	};

	cp_rst: cp-rst {
		samsung,pins = "gph1-0";
		samsung,pin-pud = <S3C64XX_PIN_PULL_NONE>;
	};

	pda_active: pda-active {
		samsung,pins = "mp03-3";
		samsung,pin-pud = <S3C64XX_PIN_PULL_NONE>;
	};

	jack_det_irq: jack-det-irq {
		samsung,pins = "gph0-6";
		samsung,pin-function = <EXYNOS_PIN_FUNC_EINT>;
	};

	media_key_irq: media-key-irq {
		samsung,pins = "gph3-6";
		samsung,pin-function = <EXYNOS_PIN_FUNC_EINT>;
	};
};

&uart3 {
	status = "okay";
};

&wm8994 {
	wlf,gpio-cfg = <0xa101 0x8100 0x8100 0x8100 0x8100 0xa101
		0x0100 0x8100 0x0100 0x0100 0x0100>;
};
